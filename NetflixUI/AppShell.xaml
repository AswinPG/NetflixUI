    x:Class="NetflixUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:home="clr-namespace:NetflixUI.Views"
    xmlns:local="clr-namespace:NetflixUI"
